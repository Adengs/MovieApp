<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <style name="Theme.Ads" parent="Theme.MaterialComponents.DayNight.NoActionBar.Bridge">
        
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/black</item>
        
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        
    </style>
    <style name="ThemeActionBar" parent="Theme.MaterialComponents.Light.NoActionBar.Bridge">
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:forceDarkAllowed" ns1:targetApi="q">false</item>
        <item name="colorAccent">@color/black</item>

    </style>
    <style name="WhiteActionBar" parent="Theme.MaterialComponents.Light.NoActionBar.Bridge">
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="android:forceDarkAllowed" ns1:targetApi="q">false</item>
        <item name="colorAccent">@color/black</item>

    </style>
</resources>